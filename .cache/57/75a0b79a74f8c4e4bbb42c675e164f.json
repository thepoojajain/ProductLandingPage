n    flex-direction: column;\r\n  }\r\n  .list li {\r\n    padding: 0 2rem;\r\n  }\r\n}\r\n\r\n/*form*/\r\nform {\r\n  margin: 0 auto;\r\n}\r\n.landing-page {\r\n  text-align: center;\r\n}\r\n\r\n.landing__heading {\r\n  margin-bottom: 2rem;\r\n  white-space: nowrap;\r\n}\r\n\r\n.email {\r\n  padding: 1rem;\r\n  width: 100%;\r\n  max-width: 275px;\r\n  border: 1px solid lightgrey;\r\n  margin: auto;\r\n}\r\n\r\n.submit {\r\n  display: block;\r\n  margin: 2rem auto;\r\n  text-transform: uppercase;\r\n  background-color: gold;\r\n  border: none;\r\n  font-weight: 700;\r\n  padding: 0.5rem 2rem;\r\n  white-space: nowrap;\r\n  cursor: pointer;\r\n  text-align: center;\r\n}\r\n\r\n.submit:hover {\r\n  background-color: #e6e200;\r\n}\r\n\r\n@media screen and (max-width: 500px) {\r\n  .email::-webkit-input-placeholder {\r\n    font-size: 1.5rem;\r\n  }\r\n  .landing__heading {\r\n    white-space: normal;\r\n  }\r\n}\r\n\r\n@media screen and (min-width: 1024px) {\r\n  body {\r\n    font-size: 1.6rem;\r\n  }\r\n}\r\n\r\n/*Feature */\r\n\r\n/*grid*/\r\n.grid {\r\n  display: grid;\r\n  margin: 3rem auto;\r\n}\r\n.grid__icon {\r\n  display: none;\r\n}\r\n.grid-content {\r\n  text-align: center;\r\n}\r\n.grid-content h2 {\r\n  margin-bottom: 0;\r\n}\r\n.grid-content p {\r\n  margin-top: 0;\r\n}\r\n@media screen and (min-width: 768px) {\r\n  .grid--3x2 {\r\n    grid-template-rows: repeat(3, 1fr);\r\n    grid-template-columns: repeat(2, 1fr);\r\n  }\r\n  .grid__icon {\r\n    display: flex;\r\n    justify-self: center;\r\n    align-self: center;\r\n    color: #fb8c00;\r\n    padding: 2rem;\r\n  }\r\n\r\n  .grid-content {\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-self: center;\r\n    width: 80vw;\r\n    text-align: left;\r\n    padding: 1rem;\r\n  }\r\n}\r\n\r\n/*video*/\r\n\r\niframe {\r\n  border: none;\r\n  margin: 0 auto;\r\n}\r\n\r\n/* Extra small devices (phones, 600px and down) */\r\n@media only screen and (max-width: 600px) {\r\n  iframe {\r\n    width: 80vw;\r\n    height: 35vh;\r\n  }\r\n}\r\n\r\n/* Small devices (portrait tablets and large phones, 600px and up) */\r\n@media only screen and (min-width: 600px) {\r\n  iframe {\r\n    width: 70vw;\r\n    height: 45vh;\r\n  }\r\n}\r\n\r\n/* Medium devices (landscape tablets, 768px and up) */\r\n@media only screen and (min-width: 768px) {\r\n  iframe {\r\n    width: 60vw;\r\n    height: 50vh;\r\n  }\r\n}\r\n\r\n/* Large devices (laptops/desktops, 992px and up) */\r\n@media only screen and (min-width: 992px) {\r\n  iframe {\r\n    width: 60vw;\r\n    height: 50vh;\r\n  }\r\n}\r\n\r\n/* Extra large devices (large laptops and desktops, 1200px and up) */\r\n@media only screen and (min-width: 1200px) {\r\n  iframe {\r\n    width: 39vw;\r\n    height: 40vh;\r\n  }\r\n}\r\n/*cards*/\r\n\r\nh1,\r\nh2,\r\nh3 {\r\n  margin: auto 0;\r\n}\r\n\r\n.cards {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-content: center;\r\n}\r\n\r\n.card {\r\n  border: 1px solid #000;\r\n  width: 100%;\r\n  margin: 5rem 2rem;\r\n  max-width: 300px;\r\n}\r\n\r\n.card__header {\r\n  background: #e0e0e0;\r\n  padding: 1rem 3rem;\r\n}\r\n\r\n.card__body {\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  align-items: center;\r\n  padding: 2rem 3rem;\r\n}\r\n\r\n.card__body .submit {\r\n  font-weight: 300;\r\n  padding: 1rem 2rem;\r\n}\r\n\r\n.card__body ul {\r\n  padding-left: 0;\r\n}\r\n\r\n@media screen and (max-width: 768px) {\r\n  .cards {\r\n    flex-direction: column;\r\n    margin: 1rem auto;\r\n  }\r\n\r\n  .card {\r\n    margin: 1rem auto;\r\n  }\r\n}\r\n/*footer*/\r\n.footer {\r\n  text-align: end;\r\n  background: #e0e0e0;\r\n  padding: 0 2rem;\r\n}\r\n\r\n.footer .copyright {\r\n  margin-top: 0;\r\n  padding-bottom: 2rem;\r\n  opacity: 0.6;\r\n}\r\n\r\n.footer__list {\r\n  display: flex;\r\n  justify-content: flex-end;\r\n  align-items: center;\r\n  margin-bottom: 0;\r\n  padding-top: 2rem;\r\n}\r\n\r\n.footer__list li {\r\n  padding: 0 1rem;\r\n}\r\n\r\n@media screen and (max-width: 500px) {\r\n  .footer__list li {\r\n    padding: 0 0.5rem;\r\n    font-size: 1.7rem;\r\n  }\r\n  .footer .copyright {\r\n    font-size: 1.5rem;\r\n    opacity: 0.6;\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"aec415d809d7451639581b6018708649","cacheData":{"env":{}}}